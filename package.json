{
  "name": "myApp",
  "version": "1.0.0",
  "private": true,
  "description": "烹饪项目",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "dev:weapp": "echo TARO_APP_API_BASE_URL=http://$(hostname -I | awk '{print $1}'):18000 > .env.development && npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type:check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@tarojs/components": "4.1.8",
    "@tarojs/helper": "4.1.8",
    "@tarojs/plugin-framework-react": "4.1.8",
    "@tarojs/plugin-platform-h5": "4.1.8",
    "@tarojs/plugin-platform-weapp": "4.1.8",
    "@tarojs/react": "4.1.8",
    "@tarojs/runtime": "4.1.8",
    "@tarojs/shared": "4.1.8",
    "@tarojs/taro": "4.1.8",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "taro-ui": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@tarojs/cli": "4.1.8",
    "@tarojs/webpack5-runner": "4.1.8",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "babel-preset-taro": "4.1.8",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.18",
    "prettier": "^3.3.0",
    "sass": "^1.60.0",
    "stylelint": "^14.4.0",
    "terser": "^5.16.8",
    "typescript": "^5.1.0",
    "webpack": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
